    }\r\n                if (data.status === 400) {\r\n                    console.log(\" one or more of the fields were not provided \")\r\n                }\r\n                if (data.status === 401) {\r\n                    console.log(\"the user with the specified email not found \")\r\n                }\r\n                \r\n               \r\n            }).catch(err => console.log(err));\r\n\r\n}\r\n\r\n\r\nReact.useEffect(() => {\r\n  tokenCheck()\r\n}, []);\r\n\r\n\r\n\r\n    return  (\r\n    <Routes>\r\n    <Route exact path=\"/\" element={<ProtectedRoute isLoggedIn={loggedIn}  email={email} component={App}/>}/>\r\n    <Route path=\"/signin\" element={<Login handelLoginSubmit={handelLoginSubmit}/>}/>\r\n      <Route path=\"/signup\" element={<Register />}/>\r\n  \r\n    </Routes>\r\n\r\n)\r\n\r\n}\r\n\r\nexport default Router\r\n\r\n\r\n","/Users/shalom/Desktop/תיקיה ללא שם 2/frontend/src/reportWebVitals.js",[],[],"/Users/shalom/Desktop/תיקיה ללא שם 2/frontend/src/components/Login.js",["106"],[],"import React, { useState } from \"react\";\r\n\r\nimport Header from \"./Header\";\r\n\r\nimport { Link, useNavigate } from \"react-router-dom\";\r\nfunction Login({ handelLoginSubmit }) {\r\n  const [values, setvalue] = useState({\r\n    email: \"\",\r\n    password: \"\",\r\n  });\r\n  function handleSubmit(e) {\r\n    e.preventDefault();\r\n    handelLoginSubmit(values);\r\n    // auth.login(email, password)\r\n    //     .then((data) => {\r\n\r\n    //         if (data.token) {\r\n    //             localStorage.setItem('jwt', data.token);\r\n    //             props.handelLogin(email)\r\n    //         }\r\n    //         if (data.status === 400) {\r\n    //             console.log(\" one or more of the fields were not provided \")\r\n    //         }\r\n    //         if (data.status === 401) {\r\n    //             console.log(\"the user with the specified email not found \")\r\n    //         }\r\n\r\n    //     }).catch(err => console.log(err));\r\n  }\